<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="POReportForSCM.controller.V_Invoice_Detail"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>InvoiceDetailPageTitle}" showNavButton="true" navButtonPress="onNavBack">
				<content>
					<List noDataText="{i18n>noData}" id="list">
						<items>
							<ObjectListItem>
								<attributes>
									<ObjectAttribute text="{i18n>belnr}: {dataModel>/Belnr}"/>
									<ObjectAttribute text="{i18n>bukrs}: {dataModel>/Bukrs}"/>
									<ObjectAttribute text="{i18n>gjahr}: {dataModel>/Gjahr}"/>
									<ObjectAttribute text="{i18n>BUZEI}: {dataModel>/Buzei}"/>
									<ObjectAttribute id="Xblnr" text="{i18n>xblnr}: {dataModel>/Xblnr}"/>
								</attributes>
							</ObjectListItem>
						</items>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>