<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="POReportForSCM.controller.V_POItem">
	<App>
		<pages>
			<Page title="{i18n>ItemPageTitle}" showNavButton="true" navButtonPress="onNavBack" navButtonTap="GoBack">
				<content>
					<Table id="it_item" items="{POItemSet}">
						<columns>
							<Column>
								<Text text="{i18n>PONumber}"/>
							</Column>
							<Column>
								<Text text="{i18n>itemTitle}"/>
							</Column>
							<Column>
								<Text text="{i18n>matnr}"/>
							</Column>
							<Column>
								<Text text="{i18n>statu}"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<Text text="{Ebeln}"/>
								<Text text="{Ebelp}"/>
								<Text text="{Matnr}"/>
								<Text text="{Statu}"/>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>