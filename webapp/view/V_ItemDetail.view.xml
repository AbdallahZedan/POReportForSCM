<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="POReportForSCM.controller.V_ItemDetail"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>ItemPageTitle}" showNavButton="true" navButtonPress="onNavBack">
				<content>
					<Table id="it_item">
						<columns>
							<Column hAlign="Center">
								<Text text="{i18n>PONumber}"/>
							</Column>
							<Column hAlign="Center">
								<Text text="{i18n>ItemNumber}"/>
							</Column>
							<Column hAlign="Center">
								<Text text="{i18n>matnr}"/>
							</Column>
							<Column hAlign="Center">
								<Text text="{i18n>statu}"/>
							</Column>
							<Column hAlign="Center">
								<Text text="{i18n>Loekz}"/>
							</Column>
							<Column hAlign="Center">
								<Text text="{i18n>txz01}}"/>
							</Column>
							<Column hAlign="Center">
								<Text text="{i18n>Ktmng}"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<Text text="{dataModel>/Ebeln}"/>
								<Text text="{dataModel>/Ebelp}"/>
								<Text text="{dataModel>/Matnr}"/>
								<Text text="{dataModel>/Statu}"/>
								<Text text="{dataModel>/Loekz}"/>
								<Text text="{dataModel>/Txz01}"/>
								<Text text="{dataModel>/Ktmng}"/>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>